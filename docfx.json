{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/DSLApp1.csproj"],
          "exclude": ["**/bin/**", "**/obj/**", "tests/**"]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["index.md"],
        "dest": "."
      },
      {
        "src": "docs",
        "files": ["**.md"],
        "exclude": ["index.md"],
        "dest": "docs"
      },
      {
        "src": "api",
        "files": ["**.yml"],
        "dest": "api"
      }
    ],
    "dest": "docs/_site",
    "globalMetadata": {
      "_appTitle": "RogueLiteDSL"
    }
  }
}
